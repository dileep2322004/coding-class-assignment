#include<stdio.h>
 int marks_sum(int marks[], int n, chargender)
{

int i, sum=0;

if(gender=='b')

for (i=0;i<n;i=1+2)

Sum=sum+marks[i];

else if(gender='g')

for(i=1;i<n;i=1+2) 
sum=sum+marks[i];

return sum;

}

void main()
{

int n,marks[100],i,sum=0;

char gender;

scanf("%d",&n);

for(i=0;i<n;i++)

scanf("%d", &marks[i]);

scanf("\n");

scanf("%c", &gender);

sum-marks_sum(marks, n, gender); 
printf("%d", sum);
}
