#include<stdio.h>
#include<string.h>
int main()
{

int a[10],len,i;
char str[30];
for(i=0;i<10;i++)
a[i]=0;
gets(str);
len=strlen(str);
for(i=0;i<len;i++)
{
	if(str[i]=='0')
	a[0]++;
	else if(str[i]=='1')
	a[1]++;
	else if(str[i]=='2')
	a[2]++;
	else if(str[i]=='3')
	a[3]++;
	else if(str[i]=='4')
	a[4]++;
	else if(str[i]=='5')
	a[5]++;
	else if(str[i]=='6')
	a[6]++;
	else if(str[i]=='7')
	a[7]++;
	else if(str[i]=='8')
	a[8]++;
	else if(str[i]=='9')
	a[9]++;
}
for(i=0;i<10;i++)
printf("%d",a[i]);
}
