#include <stdio.h>

#include <string.h>

#include <math.h>

#include <stdlib.h>

void towers(int num, char source, char target, char temp)

{

if (num==1)

{

printf("Moving ring %d from %c to %c\n", num, source, target); return;

}

towers (num-1, source, temp, target); printf("Moving ring %d from %c to %c\n", num, source, target);

towers (num-1, temp, target, source);

}

int main() {

/* Enter your code here. Read input from STDIN. Print output to S

int n;

scanf("%d", &n);

towers (n, 'A', 'B', 'C');

return 0;

= }
